{
  "name": "drupal/ethereum",
  "description": "Integrates Drupal with Ethereum.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "autoload": {
    "psr-4": {
      "Ethereum\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "package": {
        "name": "digitaldonkey/ethereum-php-lib",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/digitaldonkey/ethereum-php-lib.git"
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
    "digitaldonkey/ethereum-php-lib": "dev-master"
  }
}
